    "version": "0.2.0",
    "configurations": [
        {
            "name": "Make_sqs",
            "type": "python",
            "request": "launch",
            "module": "modules.make_sqs",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Main",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--environment",
                "DEV",
                "--xvfb",
                "False"
            ]
        }
    ]
}